{
  "name": "game-trailer-stats",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:prepare": "docker compose down && docker compose up -d && npm run pg_restore",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next export",
    "crawl": "node crawler/crawler.js",
    "crawl-only": "node crawler/crawler.js --crawlOnly",
    "prisma:migrate-dev": "npx prisma migrate dev",
    "prisma:migrate-reset": "npx prisma migrate reset",
    "prisma:migrate": "npx prisma migrate deploy",
    "pg_restore": "docker run --rm -v %cd%:/gts -w /gts --env-file .env --net game-trailer-stats_default --entrypoint \"\" postgres:14.3 sh -c \"psql -d postgresql://$POSTGRES_USER:$POSTGRES_PASSWORD@postgres:5432/$POSTGRES_DB -f ./data/data.sql\"",
    "pg_dump": "docker run --rm -v %cd%:/gts -w /gts --env-file .env --net game-trailer-stats_default --entrypoint \"\" postgres:14.3 sh -c \"pg_dump postgresql://$POSTGRES_USER:$POSTGRES_PASSWORD@postgres:5432/$POSTGRES_DB -E UTF8 -f ./data/data.sql\""
  },
  "dependencies": {
    "@googleapis/youtube": "^6.0.0",
    "@prisma/client": "^4.1.1",
    "@visx/axis": "^2.11.1",
    "@visx/event": "^2.6.0",
    "@visx/group": "^2.10.0",
    "@visx/legend": "^2.10.0",
    "@visx/marker": "^2.11.1",
    "@visx/mock-data": "^2.1.2",
    "@visx/responsive": "^2.10.0",
    "@visx/scale": "^2.2.2",
    "@visx/shape": "^2.11.1",
    "@visx/tooltip": "^2.10.0",
    "@visx/zoom": "^2.10.0",
    "babel-plugin-superjson-next": "^0.4.3",
    "d3-array": "^3.2.0",
    "d3-scale-chromatic": "^3.0.0",
    "dotenv": "^16.0.1",
    "next": "^12.2.2",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "slugify": "^1.6.5",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@types/d3-array": "^3.0.3",
    "@types/d3-scale-chromatic": "^3.0.0",
    "@types/node": "18.6.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/yargs": "^17.0.10",
    "autoprefixer": "^10.4.7",
    "eslint": "8.21",
    "eslint-config-next": "^12.2.2",
    "postcss": "^8.4.14",
    "prisma": "^4.1.1",
    "tailwindcss": "^3.1.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
